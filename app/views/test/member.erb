<h1> 회원정보 관리 창입니다 </h1>

    <div id="tf-services" class="text-center">
        <div class="container">
        <!-- 3단 선택창 -->
            <form name="form" method="get" action="/test/check">
                <div class="col-xs-12 col-md-3 col-lg-3">
                <select name="subject" onchange="change_subject(this.form)" class="form-control"></select>
                </div>
                <div class="col-xs-12 col-md-3 col-lg-3">
                <select name="group"onchange="change_group(this.form)" class="form-control"></select>
                </div>
                <div class="col-xs-12 col-md-3 col-lg-3">
                <select name="dept" class="form-control"></select>
                </div>
                <div class="col-xs-12 col-md-3 col-lg-3">
                <button type="submit" class="btn tf-btn btn-default">검색</button>
                </div>
            </form>
        </div>
    </div>