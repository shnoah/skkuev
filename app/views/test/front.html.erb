    <!-- 강의마켓
    ==========================================-->
    <div id="tf-testimonials" class="text-center">
        <div class="overlay">
            <div class="container">
            <!-- 가격입력창 -->
                <div class="section-title center">
                    <h2 style="text-align:left">
                    <span style="float:right">
                    <button type="button" class="btn tf-btn btn-default" data-toggle="modal" data-target="#myModal">팔기</button></span>
                    <strong>강의 </strong> 마켓
                    </h2>
                    <div class="line">
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-8 col-md-offset-2">
                        <div id="testimonial" class="owl-carousel owl-theme">
                        <% @sb.each do |b| %>
                            <% if b.id > 5 %>
                            <% else %>
                                <div class="item">
                                    <h5><%=b.title%> | <%=b.bunho%> | <%=b.money%>원
                                    <p><%=b.sulmung%></p></h5>
                                    <p><strong>아이디</strong></p>
                                    <p>2015.09.01.17:09</p>
                                </div>
                            <% end %>
                        <% end %>
                        </div>
                    </div>
                </div>
              <a href="/test/market_board" class="fa fa-angle-down page-scroll"></a>
            </div>
            
            <!-- Modal -->
            <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
              <div class="modal-dialog" role="document">
                <div class="modal-content">
                  <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title" id="myModalLabel">강의판매</h4>
                  </div>
                  
                  <div class="modal-body">
                  <%= form_tag("/test/market", method: "post") do %>
                    <div class="row">
                        <div class="form-group">
                          <label for="title">강의명</label>
                          <input type="text" class="form-control" name="title" placeholder="강의명">
                        </div>
                        <div class="form-group">
                          <label for="money">가격</label>                    
                          <input type="integer" class="form-control" name="money" placeholder="가격">
                        </div>
                        <div class="form-group">
                          <label for="bunho">카카오톡 아이디 or 이메일 or 핸드폰 번호</label> 
                          <input typle="email"  class="form-control" name="bunho" placeholder="010-1111-1111"></textarea>
                        </div>
                        <div class="form-group">
                          <label for="sulmung">추가설명</label> 
                          <input type="text" class="form-control" name="sulmung" placeholder="설명">
                        </div>
                        <button type="button" class="btn btn-default" data-dismiss="modal">닫기</button>
                        <button type="submit" class="btn tf-btn btn-default">올리기</button>
                      </div>
                    <% end %>
                    </div>  
                    
                    </div>
                  </div>
                </div>
          </div>
        </div>
    </div>                

